{
  "name": "express-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": "https://github.com/ismaelalvesgit/node-express-example.git",
  "author": "Ismael Alves <cearaismael1997@gmail.com>",
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "babel-node index.js",
    "dev": "nodemon --exec babel-node index.js",
    "lint": "eslint src",
    "test": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles --maxWorkers=1 --testTimeout=50000",
    "test:unit": "cross-env NODE_ENV=test jest jest --testPathPattern=test/unit --forceExit --detectOpenHandles --maxWorkers=1 --testTimeout=50000",
    "test:integration": "cross-env NODE_ENV=test jest jest --testPathPattern=test/integration --forceExit --detectOpenHandles --maxWorkers=1 --testTimeout=50000",
    "test:watch": "cross-env NODE_ENV=test jest ---watchAll",
    "coverage": "cross-env NODE_ENV=test jest --coverage --forceExit --detectOpenHandles --maxWorkers=1 --testTimeout=50000",
    "coverage:coveralls": "jest -i  --forceExit --detectOpenHandles --testTimeout=50000 cat ./coverage/lcov.info | coveralls",
    "migrate:up": "knex migrate:latest --env local",
    "migrate:down": "knex migrate:down --env local"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "keywords": [
    "node",
    "node.js",
    "express",
    "rest",
    "api",
    "knex",
    "mysql2",
    "es6",
    "es7",
    "es8",
    "es9",
    "jest",
    "docker",
    "joi",
    "eslint",
    "date-fns",
    "morgan",
    "winston-daily-rotate-file"
  ],
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "amqplib": "^0.7.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "date-fns": "^2.19.0",
    "date-fns-tz": "^1.1.3",
    "dotenv": "^8.2.0",
    "elastic-apm-node": "^3.12.1",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "hide-powered-by": "^1.1.0",
    "hsts": "^2.2.0",
    "http-status-codes": "^2.1.4",
    "knex": "^0.95.3",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "swagger-jsdoc": "^7.0.0-rc.6",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.1",
    "x-xss-protection": "^2.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.12",
    "@babel/register": "^7.13.8",
    "@babel/runtime": "^7.13.10",
    "@types/jest": "^26.0.22",
    "babel-jest": "^26.6.3",
    "chance": "^1.1.7",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  }
}
